<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btnRefreshURL"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:text="重新加载URL" />

    <!--原生方式调用扫描-->
    <Button
        android:id="@+id/btnScanNative"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:text="原生方式扫描二维码" />

    <!-- button 演示Java调用web -->
    <Button
        android:id="@+id/btnJavaCallJs"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:text="Java调用Js" />

    <!-- webview 演示web调用Java -->
    <cc.wco.jsbridge.BridgeWebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></cc.wco.jsbridge.BridgeWebView>

</LinearLayout>
